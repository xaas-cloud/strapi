{
  "name": "@strapi/provider-email-nodemailer",
  "version": "5.36.1",
  "description": "Nodemailer provider for Strapi",
  "keywords": [
    "strapi",
    "email",
    "provider",
    "nodemailer"
  ],
  "homepage": "https://strapi.io",
  "bugs": {
    "url": "https://github.com/strapi/strapi/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strapi/strapi.git"
  },
